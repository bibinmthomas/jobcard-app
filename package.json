{
  "name": "jobcard-app",
  "version": "1.0.0",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:electron\"",
    "dev:renderer": "npm run dev --prefix renderer",
    "dev:electron": "cross-env NODE_ENV=development electron-forge start",
    "build": "npm run build:renderer && electron-forge make",
    "build:renderer": "npm run build --prefix renderer",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
